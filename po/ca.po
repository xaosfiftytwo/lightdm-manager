#, python-format
msgid "The command '%(cmd)s' returned error code: %(ret)d."
msgstr "L'ordre '%(cmd)s' ha retornat el codi d'error: %(ret)d."

#: ../usr/lib/lightdm-manager/user.py:321
#, python-format
msgid ""
"Cannot remove user '%(user)s': processes started by user are still running."
msgstr ""
"No podeu eliminar l'usuari '%(user)s': els processos engegats per aquest "
"usuari encara s'executen."

